{
  "name": "PPW_Driver",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "emulators": "npx react-native run-android --list-devices",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "node ./bin/postInstall",
    "clean": "rm -rf node_modules && rm -rf yarn.lock && cd ios && rm -rf Pods && rm -rf Podfile.lock && cd .. && cd android && rm -rf build && cd app && rm -rf build && cd .. && cd ..",
    "generate_apk": "cd android && ./gradlew app:assembleRelease",
    "pod_install": "cd ios && pod install && cd ..",
    "lint": "eslint --fix \"app/**/*.{js,jsx}\"",
    "format": "prettier --write \"app/**/*.{js,jsx}\"",
    "ipad_pro": "npx react-native run-ios --simulator='iPad Pro (12.9-inch) (6th generation)'",
    "ipad_air": "npx react-native run-ios --simulator='iPad Air (5th generation)'",
    "ipad": "npx react-native run-ios --simulator='iPad (9th generation)'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/app": "^21.0.0",
    "@react-native-firebase/crashlytics": "^21.0.0",
    "@react-native-firebase/messaging": "^21.0.0",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-native-ml-kit/barcode-scanning": "^1.4.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.7",
    "i18n-js": "^4.4.3",
    "lottie-react-native": "^7.0.0",
    "moment": "^2.30.1",
    "react": "^16.8.6 ||18.3.1",
    "react-native": "^0.75.3",
    "react-native-date-picker": "^5.0.7",
    "react-native-device-info": "^13.0.0",
    "react-native-gesture-handler": "^2.20.0",
    "react-native-image-picker": "^7.1.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.8.3",
