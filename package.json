    "react-native-screens": "^3.34.0",
    "react-native-svg": "^15.7.1",
    "react-native-toast-message": "^2.2.1",
    "react-native-vision-camera": "^4.5.3",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0"
